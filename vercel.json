{
  "version": 1,
  "builds": [
    {
      "src": "src/**/*.ts",
      "use": "@vercel/typescript",
      "config": "tsconfig.json"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "HEAD", "OPTIONS"]
    }
  ]
}
